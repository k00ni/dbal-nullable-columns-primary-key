{
    "name": "k00ni/dbal-nullable-columns-primary-key",
    "description": "",
    "type": "project",
    "license": "Public Domain",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-pdo": "*",
        "doctrine/collections": "^2",
        "doctrine/common": "^3",
        "doctrine/dbal": "^4",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/orm": "^3.2",
        "doctrine/persistence": "^4",
        "symfony/console": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/routing": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/yaml": "^7.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
